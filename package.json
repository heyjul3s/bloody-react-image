{
  "name": "bloody-react-image",
  "version": "0.1.0",
  "description": "An ReactJS image component with options for fallback imagery, placeholder when loading and fallback for IE with object-fit usage.",
  "main": "lib/index.js",
  "scripts": {
    "build": "rollup -c",
    "start": "start-storybook -p 5555",
    "test": "jest",
    "test-watch": "jest --watch",
    "test-output": "jest --json --outputFile=.jest-test-results.json || true"
  },
  "files": [
    "lib"
  ],
  "peerDependencies": {
    "react": ">=16.3.0"
  },
  "keywords": [
    "bloody-react-image",
    "bloody-react-img",
    "react",
    "reactjs",
    "reactjs-image",
    "react-image",
    "react-image-placeholder",
    "react image",
    "react image placeholder",
    "bloody react image",
    "bloody react img",
    "preloader",
    "placeholder",
    "loader",
    "plus",
    "image",
    "img"
  ],
  "author": "Julian Low <julian.cy.low@gmail.com>",
  "license": "MIT",
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "lint-staged ": {
    "*.js": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!.storybook/*",
      "!src/**/*.stories.{js,jsx}"
    ],
    "moduleDirectories": [
      "node_modules",
      "bower_components"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json"
    ],
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "babel-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(ts|tsx)$",
    "verbose": false
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@babel/runtime": "^7.5.5",
    "@storybook/addon-a11y": "^5.1.9",
    "@storybook/addon-actions": "^5.1.9",
    "@storybook/addon-console": "^1.2.1",
    "@storybook/addon-jest": "^5.1.9",
    "@storybook/addon-knobs": "^5.1.9",
    "@storybook/addon-storyshots": "^5.1.9",
    "@storybook/addon-storysource": "^5.1.9",
    "@storybook/addon-viewport": "^5.1.9",
    "@storybook/react": "^5.1.9",
    "@types/react": "^16.8.23",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.3.5",
    "jest": "^24.8.0",
    "jest-cli": "^24.8.0",
    "lint-staged": "^9.2.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "rollup": "^1.17.0",
    "rollup-plugin-typescript2": "^0.22.0",
    "rollup-plugin-uglify": "^6.0.2",
    "storybook-addon-jsx": "^7.1.5",
    "storybook-readme": "^5.0.5",
    "tslib": "^1.10.0",
    "typescript": "^3.5.3"
  },
  "dependencies": {}
}
